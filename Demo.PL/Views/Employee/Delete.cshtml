@model Employee
@{
    ViewData["Title"] = "Delete";
}

<h1>Are you sure to Delete @Model.Name</h1>
<div asp-validation-summary="All"></div>
<form asp-action="Delete">

    <partial name="PartialViews/DeleteDetailsPartialView" />

    <input type="submit" value="Delete" class="btn btn-danger" /> |
    <a asp-action="Index" class="btn btn-light">Cancle</a>
</form>
